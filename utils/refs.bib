	pages = {264--269},
	file = {Full Text PDF:C\:\\Users\\stefa\\Zotero\\storage\\XB6D6GI7\\Gunasekara et al. - 2014 - Fixed effects analysis of repeated measures data.pdf:application/pdf;Snapshot:C\:\\Users\\stefa\\Zotero\\storage\\G9R7C7QA\\732283.html:text/html},
